<div class="">
  <h1>Dodaj nowe zamówienie</h1>
  <% if client_signed_in? %>
    <%= link_to("Wróć", {:controller => "client_panel", :action => "index"}) %>
  <% elsif user_signed_in? %>
    <%= link_to("Wróć", {:action => 'index'}) %>
  <% end %>

  <%= form_for(:order, :url => {:action => "create"}) do |field|%>
    <%= render(:partial => "form", :locals => {:field => field})%>
    <div >
      <%= submit_tag("Dodaj") %>
    </div>
  <% end %>
</div>
