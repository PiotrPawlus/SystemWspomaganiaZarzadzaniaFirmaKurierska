<div class="">
  <h1>Paczki</h1>
  <%= link_to("Dodaj paczkę", {:controller => 'packages', :action => 'new'})%>
  <table>
    <tr>
      <th>ID</th>
      <th>Waga</th>
      <th>Aktualny miejsce paczki</th>
      <th>Koszt</th>
      <th>Utworzono</th>
      <th>Aktualizowano</th>
      <th>Akcje</th>
    </tr>
    <% @packages.each do |package|%>
      <tr>
        <th><%= package.id %></th>
        <th><%= package.weight %></th>
        <th><%= package.actual_place %></th>
        <th><%= package.cost %></th>
        <th><%= package.created_at %></th>
        <th><%= package.updated_at %></th>
        <th>
          <%= link_to("Pokaż", {:action => 'show', :id => package.id}, :class=>"")%>
          <%= link_to("Edycja", {:action => 'edit', :id => package.id}, :class=>"")%>
          <%= link_to("Usuń", {:action => 'destroy', :id => package.id }, :class=>"")%>
        </th>
      </tr>
    <% end %>
  </table>
