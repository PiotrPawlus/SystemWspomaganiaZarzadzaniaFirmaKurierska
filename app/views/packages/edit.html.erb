<div class="">
  <h1>Edytuj paczkę nr: <%= @package.ID %></h1>

  <%= link_to("Wróć", {:action => 'index'}) %>
  <%= form_for(:package, :url => {:action => "update", :id => @package.id}) do |field|%>
    <%= render(:partial => "form", :locals => {:field => field})%>
    <div >
      <%= submit_tag("Edytuj") %>
    </div>
  <% end %>
</div>
