        <header>
            <div class="container-fluid no-padding">
                <div class="pull-left">
                    <div class="menu pull-left">
                        <i class="fa fa-bars" aria-hidden="true"></i>
                    </div>
                    <div class="logo pull-left">
                    	<%= image_tag("logo.png", alt: "Logo Kurier") %>
                    </div>
                    <a href="/orders"><p class="pull-left"><i class="fa fa-search" aria-hidden="true"></i> Znajdź paczkę</p></a>

                </div>
                <div class="pull-right">
                    <a href="/client_panel"><p class="pull-right">Menager paczek <i class="fa fa-comments" aria-hidden="true"></i></p></a>
                    <a href="/couriers"><p class="pull-right">Zamów kuriera <i class="fa fa-map-marker" aria-hidden="true"></i></p></a>
                </div>
                <div class="clearfix"></div>
            </div>
        </header>

        <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
          <!-- Indicators -->
          <ol class="carousel-indicators">
            <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
            <li data-target="#carousel-example-generic" data-slide-to="1"></li>
            <li data-target="#carousel-example-generic" data-slide-to="2"></li>
          </ol>

          <!-- Wrapper for slides -->
          <div class="carousel-inner" role="listbox">
            <div class="item active">
                <%= image_tag("home/slider-parallax1.jpg", alt: "Slide 1") %>
              <div class="slider-caption">
                  <div class="post-category">
                    <a href="">829 paczek<br>jest w drodze.</a>
                  </div>
                  <div class="slide-blog-title">
                    <a href="">Nasze statystyki <i class="fa fa-arrow-circle-o-right" aria-hidden="true"></i></a>
                  </div>
                </div>
            </div>
            <div class="item">
              <%= image_tag("home/slider-parallax2.jpg", alt: "Slide 2") %>
              <div class="slider-caption">
                  <div class="post-category">
                    <a href="">127 Państw <br>Na całym świecie.</a>
                  </div>
                  <div class="slide-blog-title">
                    <a href="">Zaufali nam <i class="fa fa-arrow-circle-o-right" aria-hidden="true"></i></a>
                  </div>
                </div>
            </div>
            <div class="item">
             <%= image_tag("home/slider-parallax3.jpg", alt: "Slide 3") %>
              <div class="slider-caption">
                  <div class="post-category">
                    <a href="">Kontakt.</a>
                  </div>
                  <div class="slide-blog-title">
                    <a href="">Infolinia <i class="fa fa-arrow-circle-o-right" aria-hidden="true"></i></a>
                  </div>
                </div>
            </div>
          </div>

            <!-- Controls -->
            <div class="slider-controls">
              <a class="slider-prev" href="#carousel-example-generic" role="button" data-slide="prev">
                <i class="fa fa-angle-left"></i>
              </a>
              <a class="slider-next" href="#carousel-example-generic" role="button" data-slide="next">
                <i class="fa fa-angle-right"></i>
              </a>
            </div>
        </div>

        <div class="find-delivery">
            <div class="container">
                <div class="row">
                    <div class="col-xs-4 number-delivery-title">
                        <h4>Wprowadź numer paczki</h4>
                        <p>Sprawdź jej dokładną lokalizację</p>
                    </div>
                    <div class="col-xs-8 no-padding">
                        <div class="col-xs-8 no-padding write-number-delivery">
                            <input type="text" placeholder="Numer paczki">
                            <button><i class="fa fa-angle-right" aria-hidden="true"></i></button>
                        </div>
                        <div class="col-xs-4 more-delivery">
                            <p>Jeżeli chcesz sprawdzić więcej niż jedną paczkę wybierz poniższą opcję.</p>
                            <a href="#">Więcej paczek <i class="fa fa-arrow-circle-o-right" aria-hidden="true"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="info-slider">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="pull-left">
                            <ul>
                            	<a href="/orders"><li>O nas</li></a>
